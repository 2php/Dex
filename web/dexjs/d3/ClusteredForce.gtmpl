<!DOCTYPE html>
<meta charset="utf-8">
<style>
  html,body, #ChartArea{
    height : 100%;
    min-height : 100%;
    width : 100%;
    min-width : 100%;
  }
  
<%
  println new File("javascript/jqueryui/latest/jquery-ui.min.css").text
  println new File("javascript/jqueryui/latest/jquery-ui.theme.min.css").text
%>
</style>
<html>
  <head>
    <title>dex.js : Clustered Force</title>
  </head>
  <body>
  <div id="ChartArea"></div>
<script>
<%
  println new File("javascript/jquery/latest/jquery-2.1.3.min.js").text
  println new File("javascript/jqueryui/latest/jquery-ui.min.js").text
  println new File("javascript/underscore/latest/underscore-min.js").text
  println new File("javascript/dexjs/latest/dex.min.js").text

  println dexData.toCsvString();
%>

var clusteredforce = dex.charts.d3.ClusteredForce({
  parent : "#ChartArea",
  resizable : true,
  groups: [{'category': 0, 'value': 2, 'label': 1}],
  color : dex.color.getColormap("crayola120"),
  csv : csv
});

clusteredforce.load();
clusteredforce.render();

function setValues(values)
{
  values.forEach(function(config) {
    switch (config.chartName) {
      case "clusteredforce":
      {
        clusteredforce.attr(config.key, config.value);
        break;
      }
    }
  });
  clusteredforce.update();
  clusteredforce.save();
}

function getGuiDefinition()
{
  var guiDef = {
    'clusteredforce': clusteredforce.getGuiDefinition()
  };

  return JSON.stringify(guiDef);
}
</script>
</body>
</html>
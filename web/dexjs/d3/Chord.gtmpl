<!DOCTYPE html>
<meta charset="utf-8">
<style>
  html,body,#ChartArea{
    height : 100%;
    min-height : 100%;
    width : 100%;
    min-width : 100%;
  }
  
<%
  println new File("javascript/jqueryui/latest/jquery-ui.min.css").text
  println new File("javascript/jqueryui/latest/jquery-ui.theme.min.css").text
%>
</style>
<html>
  <head>
    <title>DexCharts : Chord Diagram</title>
  </head>
  <body>
  <div id="config" style="visibility: hidden">
  </div>
  <div id="ChartArea"></div>
<script>
<%
  println new File("javascript/jquery/latest/jquery-2.1.3.min.js").text
  println new File("javascript/jqueryui/latest/jquery-ui.min.js").text
  println new File("javascript/underscore/latest/underscore-min.js").text
  println new File("javascript/dexjs/latest/dex.min.js").text
%>
</script>
<script>
<%
  println dexData.toCsvString();
%>

function setConfiguration(config)
{
  chord.configure(config);
  chord.update();
}

var chord = new dex.charts.d3.Chord({
  parent : "#ChartArea",
  resizable : true,
  color : d3.scale.category20c(),
  csv : csv
});

chord.load('#config');
chord.render();

</script>
</body>
</html>
<title>DexCharts : Parallel Coordinates</title>
  </head>
  <body>
  <div id="config" style="visibility: hidden"></div>
  <svg id="ChartArea"></div>
<script>

function setConfiguration(config)
{
  chart.configure(config).update();
}

<%
  println dexData.toCsvString()
%>

var chart = new dex.charts.d3.ParallelCoordinates2({
  parent : "#ChartArea",
  csv : csv,
  'brush.mode' : '1D-axes-multi',
  'foregroundQueueRate' : 100,
  'brushedQueueRate'    : 50
});

chart.render();

function setValue(key, value)
{
  chart.attr(key, value).update();
}

function getGuiDefinition()
{
  return JSON.stringify(chart.getGuiDefinition());
}

</script>
